{
  "mission_id": 1,
  "name": "Elegancyart Product Streams",
  "source_platforms": ["alibaba", "cj", "aliexpress"],

  "rules": {
    "max_per_category": 30,
    "keep_best_sellers": true,
    "auto_refresh_weekly": true,
    "no_outdoor_clothing": true,
    "removed_categories": [
      "Health & Baby Care",
      "Sports & Outdoors",
      "Office Products",
      "Industrial & Scientific"
    ]
  },

  "product_schema": {
    "id": "string - internal product id",
    "name_en": "string - English title",
    "name_fr": "string - French title",
    "slug": "string - URL friendly slug",
    "category_key": "string - matches one of the category keys below",
    "subcategory": "string - one of that category's subcategories",
    "price": "number - final sale price",
    "original_price": "number - original price before discount",
    "currency": "string - e.g. CAD, USD",
    "discount_percent": "number - optional",
    "supplier_platform": "string - alibaba | cj | aliexpress",
    "supplier_name": "string",
    "supplier_product_id": "string",
    "source_url": "string - product URL at supplier",
    "image_url": "string - main image",
    "thumbnail_url": "string - small image",
    "shipping_to": "array of ISO country codes, e.g. [\"CA\", \"US\"]",
    "shipping_days_estimate": "number - max days to deliver",
    "rating": "number - average rating",
    "review_count": "number",
    "stock_status": "string - in_stock | low_stock | out_of_stock",
    "is_best_seller": "boolean",
    "is_featured": "boolean",
    "is_supplement": "boolean - true only for supplement/health products",
    "active": "boolean",
    "created_at": "ISO datetime string",
    "updated_at": "ISO datetime string"
  },

